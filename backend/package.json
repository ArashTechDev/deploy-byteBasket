{
  "name": "bytebasket-backend",
  "version": "1.0.0",
  "description": "ByteBasket Food Bank Management System - Backend API",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "node scripts/migrate-inventory-to-mongodb.js",
    "migrate:force": "node scripts/migrate-inventory-to-mongodb.js --force",
    "migrate:dry-run": "node scripts/migrate-inventory-to-mongodb.js --dry-run",
    "backup": "node scripts/migrate-inventory-to-mongodb.js --backup-only",
    "setup:mongodb": "node src/db/models/inventory/createInventory.js",
    "setup:users": "node src/db/models/users/createUserCollection.js",
    "setup:foodbanks": "node src/db/models/foodbanks/createFoodbanks.js",
    "setup:notifications": "node src/db/models/notifications/createNotifications.js",
    "test": "jest --config tests/jest.config.js",
    "test:watch": "jest --config tests/jest.config.js --watch",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "test:compatibility": "jest --config tests/jest.config.js tests/inventory/inventory.compatibility.test.js",
    "test:integration": "jest --config tests/jest.config.js tests/inventory/inventory.integration.test.js",
    "test:inventory": "jest --config tests/jest.config.js tests/inventory/*.test.js",
    "test:runner": "node tests/run-tests.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "fast-csv": "^4.3.6",
    "exceljs": "^4.4.0",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "food-bank",
    "inventory",
    "mongodb",
    "express",
    "nodejs",
    "api"
  ],
  "author": "ByteBasket Team",
  "license": "MIT"
}